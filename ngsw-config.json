{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/*.css",
        "/*.js"
      ],
      "urls": [
        "https://fonts.googleapis.com/**"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**",
        "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
      ]
    }
  }],
  "dataGroups": [{
      "name": "Quote API",
      "urls": ["https://quotes.rest/qod.json"],
      "cacheConfig": {
        "strategy": "performance",
        "maxAge": "12h",
        "maxSize": 20
      }
    },
    {
      "name": "Weather API",
      "urls": ["https://corsproxysomik.herokuapp.com/*"],
      "cacheConfig": {
        "strategy": "performance",
        "maxAge": "30m",
        "maxSize": 20
      }
    },
    {
      "name": "Reverse Geolocation API",
      "urls": ["https://nominatim.openstreetmap.org/*"],
      "cacheConfig": {
        "strategy": "performance",
        "maxAge": "3m",
        "maxSize": 20
      }
    }
  ]

}
